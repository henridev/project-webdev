{
  "compilerOptions": {
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types",
      "./typings",
      "./node_modules/project-web-dev"
    ],
    "types": [
      "node",
      "cypress",
      "./typings"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "__mocks__/*.ts",
    "cypress",
    "cypress/support"
  ],
  "exclude": [
    "**/*.js",
  ],
}
